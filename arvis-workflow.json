{
    "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
    "creator": "jopemachine",
    "name": "mock-workflow",
    "enabled": true,
    "commands": [
        {
            "type": "keyword",
            "command": "t1",
            "title": "Test 1 - keyword",
            "subtitle": "mock-subtitle",
            "argType": "no",
            "actions": [
                {
                    "type": "clipboard",
                    "modifiers": "normal",
                    "text": "Test 1 - keyword, result"
                }
            ]
        },
        {
            "type": "scriptFilter",
            "command": "t2",
            "title": "Test 2 - scriptFilter",
            "scriptFilter": "node scriptFilter.js {query}",
            "runningSubtext": "Searching...",
            "withspace": true,
            "argType": "optional",
            "actions": [
                {
                    "modifiers": "normal",
                    "type": "args",
                    "arg": "{query}",
                    "actions": [
                        {
                            "modifiers": "normal",
                            "type": "cond",
                            "if": {
                                "cond": "5 <= 10",
                                "actions": {
                                    "then": [
                                        {
                                            "type": "clipboard",
                                            "modifiers": "normal",
                                            "text": "Test 2 - scriptFilter, result"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            ]
        },
        {
            "type": "scriptFilter",
            "command": "t3",
            "title": "Test 3 - scriptFilter + scriptFilter",
            "scriptFilter": "node scriptFilter.js {query}",
            "runningSubtext": "Searching...",
            "withspace": true,
            "argType": "optional",
            "actions": [
                {
                    "type": "scriptFilter",
                    "arg": "{query}",
                    "scriptFilter": "node scriptFilter.js {query}",
                    "runningSubtext": "Searching...",
                    "withspace": true,
                    "actions": [
                        {
                            "type": "scriptFilter",
                            "arg": "{query}",
                            "scriptFilter": "node scriptFilter.js {query}",
                            "runningSubtext": "Searching...",
                            "withspace": true,
                            "actions": [
                                {
                                    "type": "clipboard",
                                    "modifiers": "normal",
                                    "text": "Test 3 - scriptFilter + scriptFilter, result"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "keyword",
            "command": "t4",
            "title": "Test 4 - muitiple, async action chain",
            "subtitle": "",
            "argType": "no",
            "actions": [
                {
                    "modifiers": "normal",
                    "type": "script",
                    "script": "node script.js 'abc'",
                    "actions": [
                        {
                            "modifiers": "normal",
                            "type": "notification",
                            "title": "script ends 1!"
                        },
                        {
                            "modifiers": "normal",
                            "type": "notification",
                            "title": "script ends 2!"
                        },
                        {
                            "type": "clipboard",
                            "modifiers": "normal",
                            "text": "Test 4 - muitiple, async action chain, result 2"
                        }
                    ]
                },
                {
                    "modifiers": "normal",
                    "type": "clipboard",
                    "text": "Test 4 - muitiple, async action chain, result 1",
                    "actions": [
                        {
                            "modifiers": "normal",
                            "type": "notification",
                            "title": "Test 4 - muitiple, async action chain, result"
                        }
                    ]
                }
            ]
        },
        {
            "type": "keyword",
            "command": "t5",
            "title": "Test 5 - Unavailable",
            "subtitle": "",
            "script": "",
            "actions": [
                {
                    "type": "keyword",
                    "command": "",
                    "title": "",
                    "subtitle": "",
                    "script": "",
                    "actions": []
                }
            ]
        },
        {
            "type": "scriptFilter",
            "command": "t6",
            "title": "Test 6 - keyword waiting test",
            "subtitle": "",
            "scriptFilter": "node scriptFilter.js",
            "runningSubtext": "",
            "withspace": true,
            "actions": [
                {
                    "type": "keyword",
                    "command": "",
                    "title": "Title test",
                    "subtitle": "",
                    "script": "",
                    "actions": [
                        {
                            "type": "notification",
                            "modifiers": "normal",
                            "title": "Complete!",
                            "text": ""
                        }
                    ]
                }
            ]
        },
        {
            "type": "hotkey",
            "hotkey": "Double opt",
            "actions": [
                {
                    "type": "keyword",
                    "title": "Test 7 - hotkey + keyword (keyword waiting 2)",
                    "subtitle": "",
                    "script": "",
                    "actions": [
                        {
                            "type": "clipboard",
                            "modifiers": "normal",
                            "text": "Test 7 - hotkey + keyword (keyword waiting 2), result"
                        }
                    ]
                }
            ]
        },
        {
            "type": "scriptFilter",
            "command": "t8",
            "title": "Test 8 - scriptFilter with withspace false",
            "subtitle": "",
            "scriptFilter": "node scriptFilter.js",
            "runningSubtext": "a",
            "withspace": false,
            "actions": []
        },
        {
            "type": "scriptFilter",
            "command": "t9",
            "title": "Test 9 - scriptFilter with argType",
            "subtitle": "",
            "scriptFilter": "node scriptFilter.js",
            "runningSubtext": "a",
            "withspace": true,
            "argType": "no",
            "actions": []
        }
    ]
}